{deps, 
  [
    {jiffy, {git, "https://github.com/AmazeGame/jiffy.git", {tag, "1.0.9"}}}
  ]
}.

{shell, [
  % {config, "config/sys.config"},
    {apps,[amazeframe,ag_config]}
]}.

{erl_opts, [
    debug_info,
    warn_export_vars,
    warn_shadow_vars,
    warn_obsolete_guard,
    warn_export_all,
    warn_unused_import,
    warn_unused_vars,
    warn_unused_function,
    warn_bif_clash,
    warn_unused_record,
    warn_deprecated_function,
    nowarn_shadow_vars
]}.

{dist_node, [
    {setcookie, 'amazegame_cookie'},
    {name, 'amazegame@127.0.0.1'}
]}.
